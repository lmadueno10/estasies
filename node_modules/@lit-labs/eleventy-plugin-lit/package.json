    "build:demo": {
      "command": "cd demo && NODE_OPTIONS=--experimental-vm-modules eleventy",
      "dependencies": [
        "build"
      ],
      "files": [
        "demo"
      ]
    },
    "serve:demo": {
      "command": "wds --watch --node-resolve --open demo/_site/test",
      "dependencies": [
        "build:demo"
      ],
